"use strict";
//view locals
module.exports={
    name: '<%=: name %>',
    title: '<%=: name|capitalize %>',
    description: '',
    <% if(program.session){ -%>
    flash: function(){
        return this.flash;
    },
    <% } -%>
    <% if(program.auth){ -%>
    user: function(){
        return this.request.user;
